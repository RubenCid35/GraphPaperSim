@prefix mod: <https://w3id.org/mod#> .
@prefix onto: <http://www.upm.ontology.es/papers#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

onto: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

onto:IsSimilarTo a owl:ObjectProperty ;
    rdfs:label "is similar to" ;
    rdfs:domain onto:PaperSimilarity ;
    rdfs:range onto:Paper .

onto:acknowledges a owl:ObjectProperty ;
    rdfs:label "acknowledges" ;
    rdfs:domain onto:Paper ;
    rdfs:range onto:Organization,
        onto:Person,
        onto:Project .

onto:assign a owl:ObjectProperty ;
    rdfs:label "assign" ;
    rdfs:domain onto:TopicAssig ;
    rdfs:range onto:Topic .

onto:hasAuthor a owl:ObjectProperty ;
    rdfs:label "has author" ;
    rdfs:domain onto:Paper ;
    rdfs:range onto:Person .

onto:hasDOI a owl:DatatypeProperty ;
    rdfs:label "has d o i" ;
    rdfs:domain onto:Paper ;
    rdfs:range xsd:anyURI .

onto:hasKeyWord a owl:DatatypeProperty ;
    rdfs:label "has key word" ;
    rdfs:domain onto:Topic ;
    rdfs:range xsd:string .

onto:hasLanguage a owl:DatatypeProperty ;
    rdfs:label "has language" ;
    rdfs:domain onto:Paper ;
    rdfs:range xsd:string .

onto:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:domain onto:Organization,
        onto:Person ;
    rdfs:range xsd:string .

onto:hasPublicationDate a owl:DatatypeProperty ;
    rdfs:label "has publication date" ;
    rdfs:domain onto:Paper ;
    rdfs:range xsd:dateTime .

onto:hasSimilarity a owl:ObjectProperty ;
    rdfs:label "has similarity" ;
    rdfs:domain onto:Paper ;
    rdfs:range onto:PaperSimilarity .

onto:hasTitle a owl:DatatypeProperty ;
    rdfs:label "has title" ;
    rdfs:domain onto:Paper ;
    rdfs:range xsd:string .

onto:hasTopic a owl:ObjectProperty ;
    rdfs:label "has topic" ;
    rdfs:domain onto:Paper ;
    rdfs:range onto:TopicAssig .

onto:isMemberOf a owl:ObjectProperty ;
    rdfs:label "is member of" ;
    rdfs:domain onto:Person ;
    rdfs:range onto:Organization .

onto:score a owl:DatatypeProperty ;
    rdfs:label "score" ;
    rdfs:domain onto:PaperSimilarity,
        onto:TopicAssig ;
    rdfs:range xsd:float .

onto:Project a owl:Class ;
    rdfs:label "Project" .

onto:Topic a owl:Class ;
    rdfs:label "Topic" .

onto:Organization a owl:Class ;
    rdfs:label "Organization" .

onto:PaperSimilarity a owl:Class ;
    rdfs:label "Paper Similarity" .

onto:TopicAssig a owl:Class ;
    rdfs:label "Topic Assig" .

onto:Person a owl:Class ;
    rdfs:label "Person" .

onto:Paper a owl:Class ;
    rdfs:label "Paper" .

